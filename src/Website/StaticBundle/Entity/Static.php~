<?php

namespace Website\StaticBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * @ORM\Entity
 *
 * Website\StaticBundle\Entity\Static
 */
class Static
{
    /**
     * @var integer $id
     */
    private $id;

    /**
     * @var string $route
     */
    private $route;

    /**
     * @var string $title
     */
    private $title;

    /**
     * @var text $content
     */
    private $content;


    /**
     * Get id
     *
     * @return integer
     */
    public function getId()
    {
        return $this->id;
    }
}